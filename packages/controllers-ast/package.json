{
  "name": "@simply-openapi/controllers-ast",
  "version": "1.0.0",
  "description": "Typescript AST utilities for projects using @simply-openapi/controllers.",
  "main": "dist/index.js",
  "types": "dts/index.d.ts",
  "files": [
    "/dist",
    "/dts",
    "readme.md"
  ],
  "scripts": {
    "clean": "rimraf dist dts",
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.release.json",
    "test": "jest --coverage",
    "format": "prettier --write \"./src/**/*.{ts,js}\"",
    "prepublish": "npm run build"
  },
  "keywords": [
    "typescript",
    "openapi",
    "controllers",
    "simply-openapi"
  ],
  "author": "SunsetFi",
  "license": "MIT",
  "devDependencies": {
    "@simply-openapi/controllers": "^1.5.0",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2"
  }
}
